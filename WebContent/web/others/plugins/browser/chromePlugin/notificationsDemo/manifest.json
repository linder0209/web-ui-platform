{
    "name": "Notificaton Demo",
    "version": "0.1",
    "description": "Notificaton Demo",
    "permissions": ["tabs", "notifications"],
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "js": ["injectscript.js"]
        }
    ],
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_title": "Notificaton Demo",
        "default_icon": "clock.png"
    },
    "manifest_version": 2
}
