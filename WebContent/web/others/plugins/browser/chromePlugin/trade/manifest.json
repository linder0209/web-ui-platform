{
    "manifest_version": 2,
    "name": "球队竞猜交易确认单",
    "description": "球队竞猜交易确认单",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs", "http://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "css": ["stylesheet/jquery-ui.custom.css", "stylesheet/styles.css"],
            "js": ["javascript/plugin/jquery.js", "javascript/plugin/jquery-ui.js", "javascript/views/index.js"]
        }
    ]
}
